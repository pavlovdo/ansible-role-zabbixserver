                "discovery_rules": [
                    {
                        "name": "Discovery Oracle ASM volumes",
                        "key": "pyora-discovery[{$ADDRESS},{$PORT},{$DATABASE},show_asm_volumes]",
                        "delay": "1h",
                        "filter": {
                            "conditions": [
                                {
                                    "macro": "{#ASMVOLUME}",
                                    "formulaid": "A"
                                }
                            ]
                        },
                        "item_prototypes": [
                            {
                                "name": "ASM Volume Free on {#ASMVOLUME}",
                                "type": "TRAP",
                                "key": "asm_volume_free[{#ASMVOLUME}]",
                                "delay": "0",
                                "history": "1w",
                                "trends": "90d",
                                "units": "B",
                                "applications": [
                                    {
                                        "name": "Oracle ASM Volumes"
                                    }
                                ],
                                "preprocessing": [
                                    {
                                        "type": "MULTIPLIER",
                                        "parameters": [
                                            "1048576"
                                        ]
                                    }
                                ]
                            },
                            {
                                "name": "ASM Volume Size on {#ASMVOLUME}",
                                "type": "TRAP",
                                "key": "asm_volume_size[{#ASMVOLUME}]",
                                "delay": "0",
                                "history": "1w",
                                "trends": "90d",
                                "units": "B",
                                "applications": [
                                    {
                                        "name": "Oracle ASM Volumes"
                                    }
                                ],
                                "preprocessing": [
                                    {
                                        "type": "MULTIPLIER",
                                        "parameters": [
                                            "1048576"
                                        ]
                                    }
                                ]
                            },
                            {
                                "name": "ASM Volume Use on {#ASMVOLUME}",
                                "type": "TRAP",
                                "key": "asm_volume_use[{#ASMVOLUME}]",
                                "delay": "0",
                                "history": "1w",
                                "trends": "90d",
                                "value_type": "FLOAT",
                                "units": "%",
                                "applications": [
                                    {
                                        "name": "Oracle ASM Volumes"
                                    }
                                ]
                            }
                        ],
                        "trigger_prototypes": [
                            {
                                "expression": "({Oracle Pyora:asm_volume_size[{#ASMVOLUME}].last()} - {Oracle Pyora:asm_volume_free[{#ASMVOLUME}].last()}) / {Oracle Pyora:asm_volume_size[{#ASMVOLUME}].last()} *100 > {$ASMHIGH}",
                                "name": "ASM Volume {#ASMVOLUME} used > {$ASMHIGH}% ({ITEM.LASTVALUE2} / {ITEM.LASTVALUE1})",
                                "priority": "AVERAGE"
                            }
                        ]
                    },
                    {
                        "name": "Discovery Oracle tablespaces",
                        "key": "pyora-discovery[{$ADDRESS},{$PORT},{$DATABASE},show_tablespaces]",
                        "delay": "1h",
                        "filter": {
                            "conditions": [
                                {
                                    "macro": "{#TABLESPACE}",
                                    "formulaid": "A"
                                }
                            ]
                        },
                        "lifetime": "1d",
                        "item_prototypes": [
                            {
                                "name": "Tablespace percentual use on {#TABLESPACE}",
                                "type": "TRAP",
                                "key": "tablespace[{#TABLESPACE}]",
                                "delay": "0",
                                "history": "1w",
                                "trends": "90d",
                                "value_type": "FLOAT",
                                "units": "%",
                                "applications": [
                                    {
                                        "name": "Oracle Tablespaces"
                                    }
                                ],
                                "trigger_prototypes": [
                                    {
                                        "expression": "{last()}>{$HIGH}",
                                        "name": "Tablespace {#TABLESPACE} used on {$DATABASE} > {$HIGH} %",
                                        "priority": "AVERAGE"
                                    }
                                ]
                            },
                            {
                                "name": "Tablespace free on {#TABLESPACE}",
                                "type": "TRAP",
                                "key": "tablespace_abs[{#TABLESPACE}]",
                                "delay": "0",
